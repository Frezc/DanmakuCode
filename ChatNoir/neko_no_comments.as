var gameStatus;
var GAME_PLAYING = 0;
var GAME_OVER = 1;
var flag = {
	visited: [
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[]
	],
	queue: [],
	reset: function() {
		for (var i = 0; i < GRID_ROW; i++) {
			for (var j = 0; j < GRID_COLUMN; j++) {
				this.visited[i][j] = 0;
			}
		}
		this.queue = [];
	},
	visit: function(loc) {
		temp_r = loc[0];
		temp_c = loc[1];
		this.visited[temp_r][temp_c] = 1;
	},
	isVisited: function(loc) {
		temp_r = loc[0];
		temp_c = loc[1];
		return this.visited[temp_r][temp_c] == 1;
	},
	pushQueue: function(loc, dirflag) {
		this.queue.push([loc, dirflag]);
	},
	popQueue: function() {
		if (this.queue.length <= 0) {
			return null;
		} else {
			return this.queue.shift();
		}
	},
	isEmpty: function() {
		return this.queue.length == 0;
	}
};
function reset() {
	grid = [];
	for (var i = 0; i < GRID_ROW; i++) {
		grid.push([]);
		for (var j = 0; j < GRID_COLUMN; j++) {
			grid[i].push(CELL_NORMAL);
		}
	}
	var random = Math.floor(Math.random() * 9);
	cat_loc = [Math.floor(random / 3) + 4, Math.floor(random % 3) + 4];
	temp_r = cat_loc[0];
	temp_c = cat_loc[1];
	grid[temp_r][temp_c] = CELL_CAT;
	var random = Math.floor(5 + Math.random() * 7);
	for (var i = 0; i < random; i++) {
		var r = Math.floor(Math.random() * GRID_ROW);
		var c = Math.floor(Math.random() * GRID_COLUMN);
		if (grid[r][c] == CELL_BLOCK || grid[r][c] == CELL_CAT) {
			i--;
			continue;
		}
		grid[r][c] = CELL_BLOCK;
	}
	mainGameView.reset();
	gameStatus = GAME_PLAYING;
	mainGameView.updateStep(0);
}
function step() {
	flag.reset();
	var arr_index = dir_search(cat_loc);
	if (arr_index == null) {
		trace("you won");
		gameStatus = GAME_OVER;
		mainGameView.setBoardText("你赢了，\n总步数："+mainGameView.step_num);
		return;
	}
	var arr = cat_loc[0] % 2 == 1 ? arr_even : arr_odd;
	var newLoc = moveBy(cat_loc, arr[arr_index]);
	temp_r = cat_loc[0];
	temp_c = cat_loc[1];
	grid[temp_r][temp_c] = CELL_NORMAL;
	temp_r = newLoc[0];
	temp_c = newLoc[1];
	grid[temp_r][temp_c] = CELL_CAT;
	cat_loc = newLoc;
	mainGameView.catMoveTo(cat_loc[0],cat_loc[1]);
	if (isAtBorder(cat_loc)) {
		trace("game over");
		gameStatus = GAME_OVER;
		mainGameView.setBoardText("猫逃走了！！\n请重新来过");
		return;
	}
}
function dir_search(loc) {
	flag.visit(loc);
	var arr = loc[0] % 2 == 1 ? arr_even : arr_odd;
	for (var i = 0; i < arr.length; i++) {
		var temp_loc = moveBy(loc, arr[i]);
		temp_r = temp_loc[0];
		temp_c = temp_loc[1];
		if (grid[temp_r][temp_c] == CELL_NORMAL) {
			flag.pushQueue(temp_loc, i);
		}
	}
	while (!flag.isEmpty()) {
		var dir = flag.popQueue();
		flag.visit(dir[0]);
		if (isAtBorder(dir[0])) {
			return dir[1];
		}
		arr = dir[0][0] % 2 == 1 ? arr_even : arr_odd;
		for (var i = 0; i < arr.length; i++) {
			var temp_loc = moveBy(dir[0], arr[i]);
			temp_r = temp_loc[0];
			temp_c = temp_loc[1];
			if (grid[temp_r][temp_c] == CELL_NORMAL && !flag.isVisited(temp_loc)) {
				flag.pushQueue(temp_loc, dir[1]);
			}
		}
	}
	return null;
}
function isAtBorder(loc) {
	if (loc[0] == 0 || loc[0] == GRID_ROW - 1 || loc[1] == 0 || loc[1] == GRID_COLUMN - 1) {
		return true;
	} else {
		return false;
	}
}
function moveBy(loc, dir) {
	return [loc[0] + dir[0], loc[1] + dir[1]];
}
function cellClick(r, c) {
	if(gameStatus == GAME_OVER){return;}
	if (grid[r][c] == CELL_NORMAL) {
		// trace("click " + r + "," + c);
		mainGameView.updateStep();
		grid[r][c] = CELL_BLOCK;
		mainGameView.addBlock(r,c);
		step();
	}
}
