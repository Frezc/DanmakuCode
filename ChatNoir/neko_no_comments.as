var gameStatus;
var GAME_PLAYING = 0;
var GAME_OVER = 1;

var flag = {
	visited: [
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[],
		[]
	],
	queue: [],

	reset: function() {
		for (var i = 0; i < GRID_ROW; i++) {
			for (var j = 0; j < GRID_COLUMN; j++) {
				this.visited[i][j] = 0;
			}
		}

		this.queue = [];
	},

	visit: function(loc) {
		temp_r = loc[0];
		temp_c = loc[1];
		this.visited[temp_r][temp_c] = 1;
	},

	isVisited: function(loc) {
		temp_r = loc[0];
		temp_c = loc[1];
		return this.visited[temp_r][temp_c] == 1;
	},

	pushQueue: function(loc, dirflag) {
		this.queue.push([loc, dirflag]);
	},

	popQueue: function() {
		if (this.queue.length <= 0) {
			return null;
		} else {
			return this.queue.shift();
		}
	},
	isEmpty: function() {
		return this.queue.length == 0;
	}
};

function reset() {
	grid = [];
	for (var i = 0; i < GRID_ROW; i++) {
		grid.push([]);
		for (var j = 0; j < GRID_COLUMN; j++) {
			grid[i].push(CELL_NORMAL);
		}
	}

	var random = Math.floor(Math.random() * 9);
	cat_loc = [Math.floor(random / 3) + 4, Math.floor(random % 3) + 4];
	temp_r = cat_loc[0];
	temp_c = cat_loc[1];
	grid[temp_r][temp_c] = CELL_CAT;

	var random = Math.floor(5 + Math.random() * 7);
	for (var i = 0; i < random; i++) {
		var r = Math.floor(Math.random() * GRID_ROW);
		var c = Math.floor(Math.random() * GRID_COLUMN);

		if (grid[r][c] == CELL_BLOCK || grid[r][c] == CELL_CAT) {
			i--;
			continue;
		}

		grid[r][c] = CELL_BLOCK;
	}

	mainGameView.reset();

	gameStatus = GAME_PLAYING;
	mainGameView.updateStep(0);
}

function step() {
	flag.reset();
	var arr_index = dir_search(cat_loc);

	if (arr_index == null) {
		trace("you won");
		gameStatus = GAME_OVER;
		mainGameView.setBoardText(""+mainGameView.step_num);
		return;
	}

	var arr = cat_loc[0] % 2 == 1 ? arr_even : arr_odd;
	var newLoc = moveBy(cat_loc, arr[arr_index]);
	temp_r = cat_loc[0];
	temp_c = cat_loc[1];
	grid[temp_r][temp_c] = CELL_NORMAL;
	temp_r = newLoc[0];
	temp_c = newLoc[1];
	grid[temp_r][temp_c] = CELL_CAT;
	cat_loc = newLoc;
	mainGameView.catMoveTo(cat_loc[0],cat_loc[1]);

	if (isAtBorder(cat_loc)) {
		trace("game over");
		gameStatus = GAME_OVER;
		mainGameView.setBoardText("DOGE");
		return;
	}
}

function dir_search(loc) {
	flag.visit(loc);
	var arr = loc[0] % 2 == 1 ? arr_even : arr_odd;
	for (var i = 0; i < arr.length; i++) {
		var temp_loc = moveBy(loc, arr[i]);
		temp_r = temp_loc[0];
		temp_c = temp_loc[1];
		if (grid[temp_r][temp_c] == CELL_NORMAL) {
			flag.pushQueue(temp_loc, i);
		}
	}
	while (!flag.isEmpty()) {
		var dir = flag.popQueue();
		flag.visit(dir[0]);
		if (isAtBorder(dir[0])) {
			return dir[1];
		}

		arr = dir[0][0] % 2 == 1 ? arr_even : arr_odd;
		for (var i = 0; i < arr.length; i++) {
			var temp_loc = moveBy(dir[0], arr[i]);
			temp_r = temp_loc[0];
			temp_c = temp_loc[1];
			if (grid[temp_r][temp_c] == CELL_NORMAL && !flag.isVisited(temp_loc)) {
				flag.pushQueue(temp_loc, dir[1]);
			}
		}
	}

	return null;
}

function isAtBorder(loc) {
	if (loc[0] == 0 || loc[0] == GRID_ROW - 1 || loc[1] == 0 || loc[1] == GRID_COLUMN - 1) {
		return true;
	} else {
		return false;
	}
}

function moveBy(loc, dir) {
	return [loc[0] + dir[0], loc[1] + dir[1]];
}

function cellClick(r, c) {
	if(gameStatus == GAME_OVER){return;}
	if (grid[r][c] == CELL_NORMAL) {
		// trace("click " + r + "," + c);
		mainGameView.updateStep();
		grid[r][c] = CELL_BLOCK;
		mainGameView.addBlock(r,c);
		step();
	}
}

function newGameView() {
	ScriptManager.clearEl();
	ScriptManager.clearTimer();
	ScriptManager.clearTrigger();

	var gameView = {};

	gameView.radius = $.height / 670 * 25;
	gameView.margin = $.height / 670 * 10;
	gameView.offsetEven = gameView.radius + gameView.margin / 2;

	gameView.canvas = $.createCanvas({
		lifeTime: 0x7FFFFFFF
	});
	$.root.setChildIndex(gameView.canvas, $.root.numChildren - 1);

	gameView.btn = $.createButton({
		x: 0,
		y: 0,
		width: 1,
		height: 1,
		alpha: 1,
		text: "",
		parent: gameView.canvas,
		lifeTime: 0x7FFFFFFF,
		onclick: function() {
			var r = Math.floor((gameView.gridCanvas.mouseY - gameView.margin) / (gameView.margin + gameView.radius * 2));
			var offset = r % 2 == 0 ? 0 : gameView.offsetEven;
			var c = Math.floor((gameView.gridCanvas.mouseX - gameView.margin - offset) / (gameView.margin + gameView.radius * 2));
			cellClick(r, c);
		}
	});

	gameView.gridCanvas = $.createCanvas({
		x: 0,
		y: 0,
		alpha: 1,
		lifeTime: 0x7FFFFFFF,
		parent: gameView.btn
	});

	gameView.grid = [];

	for (var i = 0; i < GRID_ROW; i++) {
		gameView.grid.push([]);
		var offset = i % 2 == 0 ? 0 : gameView.offsetEven;
		for (var j = 0; j < GRID_COLUMN; j++) {
			var shape = $.createShape({
				x: (gameView.margin + gameView.radius * 2) * j + (gameView.margin + gameView.radius) + offset,
				y: (gameView.margin + gameView.radius * 2) * i + (gameView.margin + gameView.radius),
				lifeTime: 0x7FFFFFFF,
				parent: gameView.gridCanvas
			});
			shape.graphics.beginFill(0x90ee90);
			shape.graphics.drawCircle(0, 0, gameView.radius-1);
			shape.graphics.endFill();
			gameView.grid[i].push(shape);
		}
	}

	gameView.bmd_dora = Bitmap.createBitmapData(48, 48);
	gameView.bmd_bishi = Bitmap.createBitmapData(48, 48);

	gameView.bmd_dora.lock();
	gameView.bmd_bishi.lock();
	for (var i = 0; i < DOGE.length; i++) {
		gameView.bmd_dora.setPixel32(i % 48, Math.floor(i / 48), DOGE[i]);
		gameView.bmd_bishi.setPixel32(i % 48, Math.floor(i / 48), BKB[i]);
	}
	gameView.bmd_dora.unlock();
	gameView.bmd_bishi.unlock();

	gameView.imageCanvas = $.createCanvas({
		x: 0,
		y: 0,
		lifeTime: 0,
		parent: gameView.canvas
	});
	gameView.canvas.setChildIndex(gameView.imageCanvas, gameView.canvas.numChildren - 1);

	gameView.bmp_dora = Bitmap.createBitmap({
		bitmapData: gameView.bmd_dora,
		lifeTime: 0,
		scale: gameView.radius / 24,
		parent: gameView.imageCanvas
	});
	gameView.bmp_bishi = [];

	gameView.btnReset = $.createButton({
		x: $.height + 100,
		y: $.height - 50,
		text: "",
		fontsize: 25,
		lifeTime: 0x7FFFFFFF,
		parent: gameView.canvas,
		onclick: function() {
			reset();
			gameView.setBoardText("");
		}
	});
	gameView.btnReset.setStyle("fillColors", [0xffffff,0xff7777]);

	gameView.btnReset.addEventListener("mouseOver", function() {
		var g_sound = $G._("_gal_sound_over");
		if (g_sound) g_sound.play();
	});

	gameView.stepView = $.createComment(": 0", {
		x: $.height + 100,
		y: 10,
		lifeTime: 0,
		parent: gameView.canvas
	});
	gameView.stepView.border = true;
	gameView.stepView.borderColor = 0xdddddd;

	gameView.step_num = 0;

	gameView.boardView = $.createComment("\n,\n(bkb)\n(doge)",{
		x: $.height + 33,
		y: 50,
		lifeTime: 0,
		parent: gameView.canvas
	});
	gameView.boardView.border = true;
	gameView.boardView.borderColor = 0x00991d;
	gameView.boardView.fontsize = 20;
	gameView.boardView.textColor = 0x00A1D6;

	gameView.setBoardText = function(str){
		this.boardView.text = str;
		this.boardView.fontsize = 20;
		this.boardView.textColor = 0x00A1D6;
	};

	gameView.updateStep = function(n){
		if(n == undefined){
			this.step_num ++;
		}else{
			this.step_num = n;
		}
		this.stepView.text = ": "+this.step_num;
	};

	gameView.addBlock = function(r,c){
		var bishi = Bitmap.createBitmap({
			bitmapData: gameView.bmd_bishi,
			lifeTime: 0,
			scale: gameView.radius / 24,
			parent: gameView.imageCanvas
		});

		bishi.x = gameView.grid[r][c].x - gameView.radius;
		bishi.y = gameView.grid[r][c].y - gameView.radius;
		gameView.bmp_bishi.push(bishi);

		(Tween.tween(bishi, {scaleX: gameView.radius / 24, scaleY: gameView.radius / 24, x: gameView.grid[r][c].x - gameView.radius, y: gameView.grid[r][c].y - gameView.radius}, 
			{scaleX: 0, scaleY: 0, x: gameView.grid[r][c].x, y: gameView.grid[r][c].y}, 0.3)).play();
	};

	gameView.clearBlock = function(){
		while(this.bmp_bishi.length > 0){
			(this.bmp_bishi.shift()).remove();
		}
	};

	gameView.reset = function(grid) {
		gameView.clearBlock();
		for (var i = 0; i < GRID_ROW; i++) {
			for (var j = 0; j < GRID_COLUMN; j++) {
				gameView.update(i, j);
			}
		}
	};

	gameView.catMoveTo = function(r,c){
		(Tween.to(gameView.bmp_dora, {x: gameView.grid[r][c].x - gameView.radius,y: gameView.grid[r][c].y - gameView.radius}, 0.5)).play();
	};

	gameView.update = function(r, c) {
		var shape = gameView.grid[r][c];
		if (grid[r][c] == CELL_NORMAL) {
		} else if (grid[r][c] == CELL_BLOCK) {
			gameView.addBlock(r,c);
		} else if (grid[r][c] == CELL_CAT) {
			this.catMoveTo(r,c);
		}
	};

	return gameView;
}

